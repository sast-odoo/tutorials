<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="awesome_owl.card">
        <div class="card d-inline-block m-2" style="width: 18rem;">
            <div class="card-body">
                <h5 class="card-title"><t t-esc="props.title"/></h5> <!-- "props" object contains all properties passed to it by the parent -->
                <p class="card-text">
                <t t-out="props.content"/>
                </p>
            </div>
        </div>
    </t>

</templates>
